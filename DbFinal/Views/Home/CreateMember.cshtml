@model DbFinal.Models.Member
@{

    ViewBag.Title = "Üye Ekle";
}


<button type="submit" onclick="window.location.href = 'MemberList';" class="btn btn-warning float-right">Geri Dön</button>
<div class="row">

    <form method="post" action="/Home/CreateMember">

        <div class="form-group">
            <label for="member_name">Üye Adı :</label>
            <input type="text" class="form-control" name="memberName" id="member_name" />
        </div>

        <div class="form-group">
            <label for="member_surname">Üye Soyadı :</label>
            <input type="text" class="form-control" name="memberSurname" id="member_surname" />
        </div>

        <button type="submit" class="btn btn-success">Kaydet</button>
    </form>

</div>

